;----------------------------------------------------------------------------------------------
#include "msp430g2553.h"
#include "../util/constants.h"
#include "../util/ordercomand.h"
;----------------------------------------------------------------------------------------------
        NAME   WORK_BYTE
        PUBLIC MBYTE,LBYTE 
        RSEG   CODE           
;----------------------------------------------------------------------------------------------
;----------------------------------------------------------------------------------------------
MBYTE ; BYTE MAIS A ESQUERDA         
;----------------------------------------------------------------------------------------------
        mov.w      #00000h,R11
        push       R13
        mov.w      R12,R13
        rra        R12
        rra        R12
        rra        R12
        rra        R12
        rra        R12
        rra        R12
        rra        R12
        rra        R12
        mov.b      R12,R11
        mov.w      R13,R12
        pop        R13
        ret
;---------------------------------------------------------------------------------------------- 
;----------------------------------------------------------------------------------------------
LBYTE ; BYTE MAIS A DIREITA         
;----------------------------------------------------------------------------------------------
        mov.w      #00000h,R10 
        mov.b      R12,R10
        ret
;---------------------------------------------------------------------------------------------- 
;----------------------------------------------------------------------------------------------
        END  
;----------------------------------------------------------------------------------------------