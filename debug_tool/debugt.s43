;----------------------------------------------------------------------------------------------
; USADO PARA DAR SUPORTE AO DESENVOLVIMENTO
;----------------------------------------------------------------------------------------------
#include "msp430g2553.h"
#include "../util/constants.h"
;----------------------------------------------------------------------------------------------
        NAME   DEBUGT
        PUBLIC ITER_DATA_IN,CONF_BT_1_3_ITER
        RSEG   CODE           
;----------------------------------------------------------------------------------------------
;----------------------------------------------------------------------------------------------
ITER_DATA_IN ; VERIFICA ATRAVÉS DO DEBUG QUAIS AS INFORMACOES FORAM INSERIDAS NO ESPAÇO 
             ; DESTINADO NA MEMORIA
;----------------------------------------------------------------------------------------------
             push         R7
             push         R8
             mov.w        #00000h,R7
             mov.w        #00000h,R8
INIT_RANGE:  mov.w        #00208h,R7
PVER:        mov.b        @R7,R8                  
             cmp.w        #00228h,R7
             jnz          INCOK           
             jmp          EXIT_VER_MEM  
INCOK:       inc          R7
             jmp          PVER
EXIT_VER_MEM:
             pop          R8
             pop          R7
             ret
;----------------------------------------------------------------------------------------------
;----------------------------------------------------------------------------------------------
CONF_BT_1_3_ITER
;----------------------------------------------------------------------------------------------
        bis.b      #BIT3,&P1REN
        bis.b      #BIT3,&P1IE
        bis.b      #BIT3,&P1IES
        bic.b      #BIT3,&P1IFG
        ret
;----------------------------------------------------------------------------------------------
;----------------------------------------------------------------------------------------------
        END  
;----------------------------------------------------------------------------------------------